{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "astro:content": [
        "./.astro/types/content.d.ts"
      ]
    },
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "scripts"
  ]
}
